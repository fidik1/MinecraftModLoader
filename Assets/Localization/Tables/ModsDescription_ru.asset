  - m_Id: 148605918998528
    m_Localized: "\u041C\u043E\u0434 Mythical Creatures\n\n\u0421 \u043C\u043E\u0434\u043E\u043C
      Mythical Creatures \u0432 \u043C\u0438\u0440\u0435 MCPE \u043F\u043E\u044F\u0432\u0438\u0442\u0441\u044F
      \u043C\u043D\u043E\u0436\u0435\u0441\u0442\u0432\u043E \u0440\u0430\u0437\u043B\u0438\u0447\u043D\u044B\u0445
      \u0441\u0443\u0449\u0435\u0441\u0442\u0432 \u0438\u0437 \u043C\u0438\u0444\u043E\u043B\u043E\u0433\u0438\u0438
      - \u043C\u0438\u043D\u043E\u0442\u0430\u0432\u0440\u044B, \u043E\u0431\u043E\u0440\u043E\u0442\u043D\u0438,
      \u043E\u0440\u043A\u0438 \u0438 \u0434\u0440\u0443\u0433\u0438\u0435!\n\n\u0415\u0441\u043B\u0438
      \u0432\u044B \u043B\u044E\u0431\u0438\u0442\u0435 RPG-\u0438\u0433\u0440\u044B
      \u0438 \u0445\u043E\u0442\u0435\u043B\u0438 \u0431\u044B \u0434\u043E\u0431\u0430\u0432\u0438\u0442\u044C
      \u0432 MCPE \u0431\u043E\u043B\u044C\u0448\u0435 \u043C\u0435\u0445\u0430\u043D\u0438\u043A\u0438,
      \u0441\u0443\u0449\u0435\u0441\u0442\u0432 \u0438 \u043E\u0440\u0443\u0436\u0438\u044F
      \u0438\u0437 \u0442\u0430\u043A\u0438\u0445 \u0438\u0433\u0440, \u0442\u043E
      \u044D\u0442\u043E \u0434\u043E\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u0435
      \u0438\u0434\u0435\u0430\u043B\u044C\u043D\u043E \u043F\u043E\u0434\u043E\u0439\u0434\u0435\u0442
      \u0432 \u043A\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043C\u043E\u0434\u0430
      \u043D\u0430 \u043D\u043E\u0432\u044B\u0445 \u043C\u043E\u0431\u043E\u0432.
      \u0422\u0435\u043F\u0435\u0440\u044C \u0432\u043E \u0432\u0440\u0435\u043C\u044F
      \u0441\u0432\u043E\u0438\u0445 \u043F\u0440\u0438\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u0439
      \u0432\u044B \u0441\u043C\u043E\u0436\u0435\u0442\u0435 \u0432\u0441\u0442\u0440\u0435\u0442\u0438\u0442\u044C
      \u043D\u043E\u0432\u044B\u0445 \u043E\u043F\u0430\u0441\u043D\u044B\u0445 \u0432\u0440\u0430\u0433\u043E\u0432,
      \u0430 \u0441\u0430\u043C\u0430 \u0438\u0433\u0440\u0430 \u0441\u0442\u0430\u043D\u0435\u0442
      \u043D\u0435\u043C\u043D\u043E\u0433\u043E \u0441\u043B\u043E\u0436\u043D\u0435\u0435.\n\n\u041C\u0438\u043D\u043E\u0442\u0430\u0432\u0440\u044B:\n\u042D\u0442\u0438
      \u043E\u0433\u0440\u043E\u043C\u043D\u044B\u0435 \u043C\u0438\u0444\u0438\u0447\u0435\u0441\u043A\u0438\u0435
      \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0430, \u043F\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043B\u044F\u044E\u0449\u0438\u0435
      \u0441\u043E\u0431\u043E\u0439 \u0433\u0438\u0433\u0430\u043D\u0442\u0441\u043A\u0438\u0445
      \u0431\u044B\u043A\u043E\u0432 \u0441 \u0442\u044F\u0436\u0435\u043B\u044B\u043C
      \u043E\u0440\u0443\u0436\u0438\u0435\u043C \u0431\u043B\u0438\u0436\u043D\u0435\u0433\u043E
      \u0431\u043E\u044F, \u044F\u0432\u043B\u044F\u044E\u0442\u0441\u044F \u0447\u0440\u0435\u0437\u0432\u044B\u0447\u0430\u0439\u043D\u043E
