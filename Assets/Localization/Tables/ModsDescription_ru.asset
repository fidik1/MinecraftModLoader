  - m_Id: 21579919360
    m_Localized: "\u041C\u0438\u0440 Nether\n\n\u0415\u0441\u043B\u0438 \u0432\u0430\u043C
      \u043D\u0440\u0430\u0432\u0438\u0442\u0441\u044F \u043F\u0440\u043E\u0432\u043E\u0434\u0438\u0442\u044C
      \u0432\u0440\u0435\u043C\u044F \u0432 \u043D\u0438\u0436\u043D\u0435\u043C
      \u043C\u0438\u0440\u0435, \u0432\u044B\u0436\u0438\u0432\u0430\u0442\u044C
      \u0432 \u0431\u043E\u043B\u0435\u0435 \u0441\u0443\u0440\u043E\u0432\u044B\u0445
      \u0443\u0441\u043B\u043E\u0432\u0438\u044F\u0445 \u0438 \u0441\u0440\u0430\u0436\u0430\u0442\u044C\u0441\u044F
      \u0441 \u043E\u043F\u0430\u0441\u043D\u044B\u043C\u0438 \u043C\u043E\u0431\u0430\u043C\u0438,
      \u0442\u043E \u0434\u043E\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u0435 Nether
      Plus \u043E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u043E \u0432\u0430\u043C
      \u043F\u043E\u043D\u0440\u0430\u0432\u0438\u0442\u0441\u044F!\n\n\u042D\u0442\u043E\u0442
      \u043C\u043E\u0434 \u0441\u0434\u0435\u043B\u0430\u0435\u0442 \u043C\u0438\u0440
      Nether \u0435\u0449\u0435 \u0441\u043B\u043E\u0436\u043D\u0435\u0435 \u0434\u043B\u044F
      \u0432\u044B\u0436\u0438\u0432\u0430\u043D\u0438\u044F, \u0434\u043E\u0431\u0430\u0432\u0438\u0432
      \u0432 MCPE \u043C\u043D\u043E\u0436\u0435\u0441\u0442\u0432\u043E \u043D\u043E\u0432\u044B\u0445
      \u0430\u0434\u0441\u043A\u0438\u0445 \u0441\u0443\u0449\u0435\u0441\u0442\u0432!
      \u0422\u0435\u043F\u0435\u0440\u044C \u0432\u044B \u0441\u043C\u043E\u0436\u0435\u0442\u0435
      \u0432\u0441\u0442\u0440\u0435\u0442\u0438\u0442\u044C \u0430\u0434\u0441\u043A\u0438\u0445
      \u0441\u0432\u0438\u043D\u0435\u0439, \u0441\u0440\u0430\u0437\u0438\u0442\u044C\u0441\u044F
      \u0441 \u043D\u043E\u0432\u044B\u043C\u0438 \u0438\u043C\u043F\u0430\u043C\u0438
      \u0438 \u0433\u043E\u043B\u0435\u043C\u0430\u043C\u0438 \u0438\u043B\u0438
      \u0434\u043E\u0433\u043D\u0430\u0442\u044C \u0430\u0434\u0441\u043A\u0438\u0435
      \u0433\u0440\u0438\u0431\u044B. \u0410 \u043C\u043E\u0436\u0435\u0442 \u0431\u044B\u0442\u044C,
      \u0432\u044B \u0440\u0435\u0448\u0438\u0442\u0435 \u043F\u0440\u043E\u0442\u0438\u0432\u043E\u0441\u0442\u043E\u044F\u0442\u044C
      \u043D\u043E\u0432\u044B\u043C \u0431\u043E\u0441\u0441\u0430\u043C, \u0432\u044B\u0437\u0432\u0430\u0432
      \u0413\u043B\u0430\u0437 \u041A\u0442\u0443\u043B\u0445\u0443, \u0421\u0442\u0435\u043D\u0443
      \u041F\u043B\u043E\u0442\u0438 \u0438\u043B\u0438 \u0418\u043D\u0444\u0435\u0440\u043D\u0430\u043B\u044C\u043D\u043E\u0433\u043E
      \u0413\u043E\u043B\u0435\u043C\u0430?\n\n\u0427\u0435\u043C \u0431\u044B \u0432\u044B
      \u043D\u0438 \u0440\u0435\u0448\u0438\u043B\u0438 \u0437\u0430\u043D\u044F\u0442\u044C\u0441\u044F
      \u0432 \u043F\u043E\u0434\u0437\u0435\u043C\u043D\u043E\u043C \u043C\u0438\u0440\u0435,
      \u0441 \u0434\u043E\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u0435\u043C Nether
      Plus \u044D\u0442\u043E \u0431\u0443\u0434\u0435\u0442 \u043D\u0430\u043C\u043D\u043E\u0433\u043E
      \u0438\u043D\u0442\u0435\u0440\u0435\u0441\u043D\u0435\u0435! \u041C\u043D\u043E\u0436\u0435\u0441\u0442\u0432\u043E
      \u043D\u043E\u0432\u044B\u0445 \u043C\u043E\u0431\u043E\u0432 \u0438\u0434\u0435\u0430\u043B\u044C\u043D\u043E
      \u0432\u043F\u0438\u0441\u044B\u0432\u0430\u044E\u0442\u0441\u044F \u0432 \u0430\u0442\u043C\u043E\u0441\u0444\u0435\u0440\u0443
      \u0438\u0433\u0440\u044B, \u0441\u043B\u043E\u0432\u043D\u043E \u0441\u043E\u0437\u0434\u0430\u043D\u043D\u044B\u0435
      \u0441\u0430\u043C\u0438\u043C\u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043E\u0442\u0447\u0438\u043A\u0430\u043C\u0438."
